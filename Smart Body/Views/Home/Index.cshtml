<!-- Start Section Subscribe -->
<section class="subscribe">
    <div class="container text-center wow fadeInUp" data-wow-duration="1s">
        <h2 class="h1">Keep In Touch</h2>
        <p class="lead">Subscribe To Get All New Offers, Dont Worry About Spam We Hate It Too.</p>
        
        <div class="form-inline col-md-6 col-md-offset-3">
            <input id="submail" class="form-control input-lg" type="text" placeholder="Write Your Email">
            <button id="subBtn" class="btn btn-primary btn-lg">Subscribe</button>
        </div>
    </div>
</section>
<!-- End Section Subscribe -->
<!-- Strat Section Contact Us -->
<section class="contact-us text-center">
    <div class="fields">
        <div class="container">
            <div class="row contact">
                <i class="glyphicon glyphicon-phone-alt" style="font-size: 55px; padding-top: 20px;"></i>
                <h2 class="h1">Tell Us What You Fell</h2>
                <p class="lead">Feel Free To Contact Us Anytime</p>
                <!-- Start Contact Form -->
                <div class="col-md-6 wow bounceInLeft" data-wow-duration="1s">
                    <div class="form-group">
                        <input id="UserContact" type="text"  class="form-control input-lg" placeholder="Username" />
                    </div>
                    <div class="form-group">
                        <input id="EmailContact" type="text"  class="form-control input-lg" placeholder="Email">
                    </div>
                    <div class="form-group">
                        <input id="PhoneContact" type="text"  class="form-control input-lg" placeholder="Phone Number">
                    </div>
                </div>
                <div class="col-md-6 wow bounceInRight" data-wow-duration="1s">
                    <div class="form-group">
                        <textarea id="MsgContact"  class="form-control input-lg" style="height: 95px;" placeholder="Your Message ..."></textarea>
                    </div>
                    <button ID="SendBtn"  type="button" class="btn btn-primary btn-lg btn-block"  data-toggle="modal" data-target="#Package" >Contact Us</button>
                </div>

                <!-- End Contact Form -->

            </div>
        </div>
    </div>
</section>
<!-- End Section Contact Us -->